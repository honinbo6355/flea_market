        <script type="text/javascript">
	        /* 카테고리, 서브 카테고리 리스트 생성 */
			var jsonCategoryArray = JSON.parse('<c:out value="${JSON_CATEGORY_ARRAY}" escapeXml="false" />');
			
			var subCategoryUl = document.createElement("ul");
			var mobileSubCategoryUl = document.createElement("ul");
			
			subCategoryUl.className = "main__menu";
			
			for (var i=0; i<jsonCategoryArray.length; i++) {
				if (jsonCategoryArray[i].cateLevel === 1) {
					$(subCategoryUl).append("<li id='" + jsonCategoryArray[i].cateCode + "' class='drop'>" 
						+ "<a href='categoryMain.do?categoryId=" + jsonCategoryArray[i].cateCode + "'>" 
						+ jsonCategoryArray[i].cateName + "</a><ul class='dropdown'></ul></li>");
					
					$('#sub-category-menu-list').append(subCategoryUl);
					
					/* 모바일 버전 메뉴 */
					$(mobileSubCategoryUl).append("<li id='m-" + jsonCategoryArray[i].cateCode + "'>"
						+ "<a href='categoryMain.do?categoryId=" + jsonCategoryArray[i].cateCode + "'>" 
						+ jsonCategoryArray[i].cateName + "</a><ul></ul></li>");		
					
					$('#mobile_dropdown').append(mobileSubCategoryUl);
					
				} else if (jsonCategoryArray[i].cateLevel === 2) {
					$('#' + jsonCategoryArray[i].upperCateCode).find(".dropdown").append(
						"<li id='" + jsonCategoryArray[i].cateCode + "'>"
						+ "<a href='categoryMain.do?categoryId=" + jsonCategoryArray[i].cateCode + "'>"
						+ jsonCategoryArray[i].cateName + "</a></li>");
					
					/* 모바일 버전 메뉴 */
					$('#m-' + jsonCategoryArray[i].upperCateCode).find("ul").append(
						"<li id='m-" + jsonCategoryArray[i].cateCode + "'>"
						+ "<a href='categoryMain.do?categoryId=" + jsonCategoryArray[i].cateCode + "'>"
						+ jsonCategoryArray[i].cateName + "</a></li>");
				}
			}
        </script>